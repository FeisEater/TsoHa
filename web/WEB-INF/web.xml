<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
    <servlet>
        <servlet-name>test</servlet-name>
        <servlet-class>QuesAns.Servlets.TestServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Login</servlet-name>
        <servlet-class>QuesAns.Servlets.LoginServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Listing</servlet-name>
        <servlet-class>QuesAns.Servlets.ListServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Ask</servlet-name>
        <servlet-class>QuesAns.Servlets.AskServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Logout</servlet-name>
        <servlet-class>QuesAns.Servlets.LogoutServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>QuestionServlet</servlet-name>
        <servlet-class>QuesAns.Servlets.QuestionServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AnswerServlet</servlet-name>
        <servlet-class>QuesAns.Servlets.AnswerServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>FlagServlet</servlet-name>
        <servlet-class>QuesAns.Servlets.FlagServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ListQuestionsServlet</servlet-name>
        <servlet-class>QuesAns.Servlets.moderator.ListQuestionsServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>RemoveQuestionServlet</servlet-name>
        <servlet-class>QuesAns.Servlets.moderator.RemoveQuestionServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>RegisterServlet</servlet-name>
        <servlet-class>QuesAns.Servlets.RegisterServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SettingsServlet</servlet-name>
        <servlet-class>QuesAns.Servlets.RegisteredUser.SettingsServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>test</servlet-name>
        <url-pattern>/test</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Login</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Listing</servlet-name>
        <url-pattern>/index</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Ask</servlet-name>
        <url-pattern>/ask</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Logout</servlet-name>
        <url-pattern>/logout</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>QuestionServlet</servlet-name>
        <url-pattern>/question</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AnswerServlet</servlet-name>
        <url-pattern>/answer</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>FlagServlet</servlet-name>
        <url-pattern>/flag</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ListQuestionsServlet</servlet-name>
        <url-pattern>/modquestions</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>RemoveQuestionServlet</servlet-name>
        <url-pattern>/modremovequestion</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>RegisterServlet</servlet-name>
        <url-pattern>/register</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SettingsServlet</servlet-name>
        <url-pattern>/accsettings</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>index</welcome-file>
    </welcome-file-list>
</web-app>
